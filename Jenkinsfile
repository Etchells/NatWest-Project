pipeline {
    agent any
    stages {
        stage('Clone Repo') {
            steps {
                echo "Clone Repo"
            }
        }
        stage('Build Jar') {
            steps {
                echo "Jar file is built here"
            }
        }
        stage('Run Jar') {
            steps {
                echo "The jar file runs here"
            }
        }
    }
}
